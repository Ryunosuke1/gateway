    "bn.js": "5.2.1",
    "web3-utils": "1.7.3",
    "brotli": "1.3.2",
    "minimist": "^0.2.4",
    "mkdirp": "^1.0.4",
    "@openzeppelin/contracts": "^4.9.6",
    "pbkdf2": "^3.1.3",
    "axios": "^1.8.4"
  },
  "files": [
    "/dist"
  ],
  "lint-staged": {
    "{src,test}/**/*.{ts,js}": "eslint --fix",
    "**/*.json": [
      "prettier --write"
    ]
  }
}
